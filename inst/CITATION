citHeader("To cite pseval in publications use:")

year = sub('.*(2[[:digit:]]{3})-.*', '\\1', meta$Date, perl = TRUE)
vers = paste('R package version', meta$Version)

citEntry(entry = "Manual",
 title = paste('pseval:', meta$Title),
  author = 'Michael C. Sachs and Erin E. Gabriel',
  year = year,
  note = vers,
  url = meta$URL,
  textVersion = paste('Michael C. Sachs and Erin E. Gabriel (', year, '). pseval: ', meta$Title, '. ', vers, '.', sep = '')
)